{
  "name": "tandem.io",
  "version": "0.0.1",
  "description": "Minimal, open-source version of TK Sync",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fauntleroy/tandem.io.git"
  },
  "author": "Timothy Kempf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Fauntleroy/tandem.io/issues"
  },
  "dependencies": {
    "async": "~0.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "classnames": "^1.1.4",
    "clean-css": "^3.4.8",
    "connect-mongo": "~0.4.0",
    "connect-redis": "^2.1.0",
    "domready": "^1.0.7",
    "express": "^3.18.4",
    "express-expose": "~0.3.3",
    "express-hbs": "~0.7.10",
    "express-session": "^1.9.2",
    "flux": "^2.0.1",
    "jsonp": "^0.1.0",
    "keymirror": "^0.1.1",
    "less": "^2.5.3",
    "lodash": "^3.3.1",
    "mkdirp": "^0.5.1",
    "mongoose": "~3.8.6",
    "node-uuid": "~1.4.1",
    "npm": "^3.5.2",
    "npm-run-all": "^1.4.0",
    "passport": "^0.2.1",
    "passport-google-oauth": "~0.1.5",
    "passport-soundcloud": "~0.1.2",
    "react": "^0.14.0",
    "react-dnd": "^2.0.2",
    "react-dnd-html5-backend": "^2.0.0",
    "react-dom": "^0.14.0",
    "react-player": "^0.3.1",
    "react-simple-dropdown": "0.1.x",
    "react-textarea-autosize": "^3.1.0",
    "request": "~2.33.0",
    "sails-mysql": "^0.10.7",
    "socket.io": "^1.3.7",
    "socket.io-client": "^1.3.7",
    "store": "~1.3.14",
    "trash-cli": "^1.2.0",
    "underscore": "^1.7.0",
    "visibilityjs": "~1.2.0",
    "watch": "^0.17.1",
    "watchify": "^3.6.1",
    "waterline": "^0.10.14",
    "xhr": "^2.0.1"
  },
  "scripts": {
    "build": "mkdirp compiled && npm-run-all build:*",
    "build:css": "lessc assets/styles/index.less assets/compiled/index.css",
    "build:js": "browserify -t [ babelify --presets [ es2015 stage-0 react ] ] --extension=.jsx assets/scripts/room.jsx --outfile assets/compiled/room.js",
    "minify": "npm-run-all minify:*",
    "minify:css": "cleancss assets/compiled/index.css -o assets/compiled/index.css",
    "minify:js": "uglifyjs assets/compiled/room.js --screw-ie8 -o assets/compiled/room.js",
    "watch": "mkdirp compiled && npm-run-all -p watch:css watch:js server",
    "watch:assets": "npm-run-all -p watch:css watch:js",
    "watch:css": "watch \"npm run build:css\" assets/styles",
    "watch:js": "watchify -t [ babelify --presets [ es2015 stage-0 react ] ] --extension=.jsx assets/scripts/room.jsx --outfile assets/compiled/room.js -v",
    "server": "node index.js",
    "clean": "trash compiled",
    "postinstall": "npm-run-all build minify"
  }
}
